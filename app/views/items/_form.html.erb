<%= form_for @item do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :price %><br>
    <%= f.text_field :price %>
  </p>
  <p>
    <%= f.label :category_id %><br>
    <%= f.text_field :category_id %><br>
  </p>
 
   <p>
    <%= f.submit %>
  </p>
<% end %>